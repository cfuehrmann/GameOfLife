{
  "compilerOptions": {
    "target": "es5",
    "module": "amd",
    "declaration": true,
    "noImplicitAny": true,
    "outDir": "BuildOutput",
    "listFiles": true
  },
    "filesGlob": [
        "*.ts"
    ],
    "exclude": [
        "node_modules",
        "BuildOutput",
        "Imports"
    ]
}
