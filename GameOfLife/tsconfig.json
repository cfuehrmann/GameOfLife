    "files": [
        "./Imports/Core/Arrays.d.ts",
        "./Imports/Core/Exceptions.d.ts",
        "./Imports/Core/Numbers.d.ts",
        "./Imports/Core/Sequences.d.ts",
        "./Imports/Core/TypeChecking.d.ts",
        "./Imports/QUnit/qunit.d.ts",
        "./Interfaces.ts",
        "./CanvasRenderer.ts",
        "./CanvasRenderer_test.ts",
        "./GameOfLifeTransformer.ts",
        "./GameOfLifeTransformer_test.ts",
        "./Start.ts",
        "./World.ts"
    ]
}
